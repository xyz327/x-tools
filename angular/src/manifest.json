{
  "manifest_version": 3,
  "name": "x-tools",
  "short_name": "x-tools",
  "version": "1.2.0",
  "description": "a tools extension for common developer",
  "icons": {
    "16": "assets/logo16x16.png",
    "32": "assets/logo32x32.png",
    "48": "assets/logo48x48.png",
    "128": "assets/logo128x128.png"
  },
  "action": {
    "default_popup": "index.html?#/popup",
    "default_icon": "assets/logo128x128.png",
    "default_title": "x-tools"
  },
  "options_page": "",
  "homepage_url": "https://github.com/xyz327/x-tools",
  "background": {

  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  }
}
